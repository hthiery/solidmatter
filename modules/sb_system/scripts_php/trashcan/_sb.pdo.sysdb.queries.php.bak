// path cache
//------------------------------------------------------------------------------

$_QUERIES['sb_system/cache/paths/store'] = '
	INSERT INTO	'.TABLE_PREFIX.'_system_cache_paths
				(
					s_path,
					fk_node
				) VALUES (
					:path,
					:node_id
				)
	ON DUPLICATE KEY UPDATE
				fk_node = :node_id
';
$_QUERIES['sb_system/cache/paths/load'] = '
	SELECT		fk_node
	FROM		'.TABLE_PREFIX.'_system_cache_paths
	WHERE		s_path = :path
';
$_QUERIES['sb_system/cache/paths/clear'] = '
	DELETE FROM	'.TABLE_PREFIX.'_system_cache_paths
	WHERE		s_path LIKE :path
';
$_QUERIES['sb_system/cache/paths/empty'] = '
	DELETE FROM '.TABLE_PREFIX.'_system_cache_paths
';

