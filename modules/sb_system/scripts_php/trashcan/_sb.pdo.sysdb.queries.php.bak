$_QUERIES['sb_system/session/load'] = '
	SELECT		s_data
	FROM		'.TABLE_PREFIX.'_system_sessions
	WHERE		s_sessionid = :session_id
';
$_QUERIES['sb_system/session/store'] = '
	INSERT INTO '.TABLE_PREFIX.'_system_sessions
				(
					s_sessionid,
					s_data
				) VALUES (
					:session_id,
					:data
				)
	ON DUPLICATE KEY UPDATE
				s_data = :data
';
$_QUERIES['sb_system/session/destroy'] = '
	DELETE FROM	'.TABLE_PREFIX.'_system_sessions
	WHERE		s_sessionid = :session_id
';

//------------------------------------------------------------------------------
// nodefactory
//------------------------------------------------------------------------------

$_QUERIES['sb_system/factory/node/load_nodetypes'] = '
	SELECT		*
	FROM		'.TABLE_PREFIX.'_system_nodetypes
';
$_QUERIES['sb_system/factory/node/by_id'] = '
	SELECT		sn.uuid,
				sn.s_uid,
				sn.fk_nodetype,
				sn.s_name,
				sn.s_label,
				sn.s_customcsstype,
				snt.s_classfile,
				snt.s_class,
				snt.s_csstype,
				(SELECT		fk_parent
					FROM	'.TABLE_PREFIX.'_system_nodes_parents
					WHERE	fk_child = :id
						AND	b_primary = \'TRUE\'
				) as fk_parent
	FROM		'.TABLE_PREFIX.'_system_nodes sn
	INNER JOIN	'.TABLE_PREFIX.'_system_nodetypes snt
		ON		sn.fk_nodetype = snt.s_type
	WHERE		sn.uuid = :id
';
$_QUERIES['sb_system/factory/node/by_uid'] = '
	SELECT		sn.uuid,
				sn.s_uid,
				sn.fk_nodetype,
				sn.s_name,
				sn.s_label,
				sn.s_customcsstype,
				snt.s_classfile,
				snt.s_class,
				snt.s_csstype,
				(SELECT		fk_parent
					FROM	'.TABLE_PREFIX.'_system_nodes_parents
					WHERE	fk_child = sn.uuid
						AND	b_primary = \'TRUE\'
				) as fk_parent
	FROM		'.TABLE_PREFIX.'_system_nodes sn
	LEFT JOIN	'.TABLE_PREFIX.'_system_nodetypes snt
		ON		sn.fk_nodetype = snt.s_type
	WHERE		sn.s_uid = :uid
';

